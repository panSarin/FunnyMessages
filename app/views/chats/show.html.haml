.container
  .row
    .col-md-8
      Messages:
      #messages
    .col-md-4
      Other users:
      #users
        %ul
  .row
    = simple_form_for @message, remote: true do |f|
      .form-group
        = f.input :content, as: :string, label: "#{session['user']['name']}: "
        = f.submit 'Send my message', class: 'btn primary-btn'
